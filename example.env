# Example environment file for CodeRun deployments
# This file shows how to configure environment variables for your application

# Database configuration
DATABASE_URL=postgresql://user:password@localhost:5432/myapp
DATABASE_MAX_CONNECTIONS=10

# Redis configuration
REDIS_URL=redis://localhost:6379
REDIS_MAX_CONNECTIONS=5

# Application settings
APP_ENV=production
APP_DEBUG=false
APP_PORT=8080
APP_LOG_LEVEL=info

# API Keys (use quotes for values with special characters)
API_KEY="your-secret-api-key-here"
JWT_SECRET="your-jwt-secret-key-here"

# Third-party services
STRIPE_PUBLIC_KEY=pk_live_your_stripe_public_key_here
STRIPE_SECRET_KEY=sk_live_your_stripe_secret_key_here

# Feature flags
FEATURE_NEW_UI=true
FEATURE_BETA_FEATURES=false
